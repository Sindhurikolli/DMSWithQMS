<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="com.mycompany_DmsSeleniumMavenApp_jar_1.0-SNAPSHOT">
   <listeners> 
        <listener class-name="com.pss.dms.Listerners.TestListener"/>
        <listener class-name="com.pss.dms.Listerners.SuiteFailingInvokedMethodListener"/>
</listeners> 
    
    <parameter name="DOCUMENT_NAME_SOP_DOCT_REQUEST" value="${DOCUMENT_NAME_SOP_DOCT_REQUEST}"/>
	<parameter name="DOCT_NO_INITIATE_NEW_DOCT_SOP" value="${DOCT_NO_INITIATE_NEW_DOCT_SOP}"/>
	
 <test name="DMS PropertiesUpdate">
    <classes>           
           <class name="com.pss.dms.PropertiesFileUpdate.UpdateDMSProperties"/>
          
	</classes>
 </test>      
    
   <test name="DMSChange Request to Approval">
        <classes>
            <class name="com.pss.dms.DMSRejectionFlow.ChangeDocumentRequest"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocumentRequestReject"/>  
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocumentRequestResubmit"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocumentRequestApproval"/>
         </classes>
    </test> 
   
	 <test name="DMSInitiate to DHodReview">
        <classes>
            <class name="com.pss.dms.DMSRejectionFlow.ChangeDoctInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDeptReviewerReject"/>  
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocReInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeQAReviewerReject"/>
         </classes>
    </test> 
	 <test name="DMSQAReviewtoCFTReject">
        <classes>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocReInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeQAReviewer"/> 
          
            </classes>
    </test> 
	 
	 <test name="DMSQAApprover">
        <classes>
       		<class name="com.pss.dms.DMSRejectionFlow.ChangeQAApproverReject"/> 
           <class name="com.pss.dms.DMSRejectionFlow.ChangeQAReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeQAApprover"/>
           </classes>
    </test>
   
    
	 <test name="DMS TrainingNotificationToComplete">
        <classes>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeNoTrainingNotificationList"/> 
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocumentTrainingApprovalReject"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeNoTrainingNotificationListAfterReject"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeDocTrainingApproval"/>
           <class name="com.pss.dms.DMSRejectionFlow.ChangeTrainingCompletedList"/>
         </classes>
    </test>
    
    
    
