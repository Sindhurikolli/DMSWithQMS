<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="com.mycompany_DmsSeleniumMavenApp_jar_1.0-SNAPSHOT">
   <listeners> 
        <listener class-name="com.pss.dms.Listerners.TestListener"/>
        <listener class-name="com.pss.dms.Listerners.SuiteFailingInvokedMethodListener"/>
</listeners> 
     <parameter name="DocumentType" value="${DocumentType}"/>
     <parameter name="ProductCode" value="${ProductCode}"/>  
   
  
     <test name="DMS Properties File Update">
        <classes>
           <class name="com.pss.dms.PropertiesFileUpdate.UpdateDMSProperties"> 
          	<methods>
				<include name="TerminateDocument"/>
			</methods>
			</class>
        </classes>
    </test>  
    <test name="CCWithDMSAndLMS Initiation to DHod">
        <classes>
           <class name="com.pss.dms.CCRejectionFlow.CCInitiation"/>
           <class name="com.pss.dms.CCRejectionFlow.DeptReviewerReject"/> 
           <class name="com.pss.dms.CCRejectionFlow.ReInitiateDocument"/>
           <class name="com.pss.dms.CCRejectionFlow.DeptReviewerApprove"/>
         </classes>
    </test> 
	 <test name="CCWithDMSAndLMS QAReviewer">
        <classes>           
           <class name="com.pss.dms.CCRejectionFlow.QAReviewReject"/>
           <class name="com.pss.dms.CCRejectionFlow.ReInitiateDocument"/>
           <class name="com.pss.dms.CCRejectionFlow.DeptReReview"/> 
           <class name="com.pss.dms.CCRejectionFlow.QAReviewApproveAfterReject"/>
         </classes>
    </test> 
	 <test name="CCWithDMSAndLMS CFT and RA Reject">
        <classes> 
           <class name="com.pss.dms.CCRejectionFlow.CFTReviewReject"/>
           <class name="com.pss.dms.CCRejectionFlow.QAReviewAfterCFTReject"/>
           <class name="com.pss.dms.CCRejectionFlow.CFTReviewApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.RegReviewerReject"/>
     	 </classes>
    </test>  
	 <test name="CCWithDMSAndLMS CFTRACustomerReject">
        <classes> 
           <class name="com.pss.dms.CCRejectionFlow.QAReviewAfterCFTReject"/>
           <class name="com.pss.dms.CCRejectionFlow.CFTReviewApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.RegReviewerApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.CustomerNotification"/>
           <class name="com.pss.dms.CCRejectionFlow.CustomerResponseReject"/>
            </classes>
    </test>  
	 <test name="CCWithDMSAndLMS CFTRACustomerApprove">
        <classes> 
           <class name="com.pss.dms.CCRejectionFlow.QAReviewAfterCustomerReject"/>
           <class name="com.pss.dms.CCRejectionFlow.CFTReviewApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.RegReviewerApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.CustomerNotification"/>
           <class name="com.pss.dms.CCRejectionFlow.CustomerResponse"/>
            </classes>
    </test> 
	 <test name="CCWithDMSAndLMS QAApproverReject">
        <classes> 
           <class name="com.pss.dms.CCRejectionFlow.QAReviewWithOutCFT"/>
           <class name="com.pss.dms.CCRejectionFlow.QAApproverReject"/>
     </classes>
    </test> 
	 <test name="CCWithDMSAndLMS QAApprover">
        <classes>        
           <class name="com.pss.dms.CCRejectionFlow.QAReviewAfterApproveReject"/>
           <class name="com.pss.dms.CCRejectionFlow.QAApprover"/>
         </classes>
    </test>   
	 <test name="CCWithDMSAndLMS PostApproval Actions Initiate">
        <classes> 
           <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionInitiateTerminateDocument"/> 
           <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionReviewReject"/>
       </classes>
    </test> 
	  <test name="CCWithDMSAndLMS PostApproval Actions Review">
        <classes>      
           <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionInitiateAfterReject"/>
              <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionReviewApprove"/>  
                <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionApprovalReject"/> 
      </classes>
    </test> 
	 <test name="CCWithDMSAndLMS PostApproval Actions Approve">
        <classes>        
         
           <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionReviewApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.PostApprovalActionApproval"/>
         </classes>
    </test> 
	  <test name="CCWithDMSAndLMS DMSInitiate to DHodReview">
        <classes>
            <class name="com.pss.dms.DMSRejectionFlow.TerminateDoctInitiate"/>
        <class name="com.pss.dms.DMSRejectionFlow.TerminateDeptReviewerReject"/>  
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDoctReInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAReviewerReject"/>
         </classes>
    </test> 
	 <test name="CCWithDMSAndLMS DMSQAReviewtoCFTReject">
        <classes>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDoctReInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAReviewer"/> 
           <class name="com.pss.dms.DMSRejectionFlow.TerminateCFTReviewerReject"/>
            </classes>
    </test> 
	 <test name="CCWithDMSAndLMS DMSQAReviewtoCFTApprove">
        <classes>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDoctReInitiate"/> 
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateCFTReviewer"/>
          
            </classes>
    </test>
	 <test name="CCWithDMSAndLMS DMSQAApprover">
        <classes>
       		<class name="com.pss.dms.DMSRejectionFlow.TerminateQAApproverRevert"/> 
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDoctReInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateCFTReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAApprover"/>
           </classes>
    </test>
	 <test name="CCWithDMSAndLMS DMSCQAApprover">
        <classes>
       	   <class name="com.pss.dms.DMSRejectionFlow.TerminateCQAApproverRevert"/> 
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDoctReInitiate"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateDeptReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateCFTReviewer"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateQAApprover"/>
           <class name="com.pss.dms.DMSRejectionFlow.TerminateCQAApprover"/>
           </classes>
    </test>
	<test name="CCWithDMSAndLMS ActionItemPerformandReview">
        <classes>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemPerform"/>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemReviewerRevert"/>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemPerformAfterReject"/>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemReview"/>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemApproveReject"/>
            </classes>
    </test> 
	 <test name="CCWithDMSAndLMS ActionItemReviewAndApprove">
        <classes>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemPerformAfterReject"/>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemReview"/>
           <class name="com.pss.dms.CCRejectionFlow.ActionItemApprove"/>
           <class name="com.pss.dms.CCRejectionFlow.CCClosureReview"/>
            </classes>
    </test> 
	 <test name="CCWithDMSAndLMS CCClosure ReviewAndApprove">
        <classes>
           <class name="com.pss.dms.CCRejectionFlow.CCclosureApproveRevert"/>
           <class name="com.pss.dms.CCRejectionFlow.CCClosureReview"/>
           <class name="com.pss.dms.CCRejectionFlow.CCclosureApprove"/>      
                      

        </classes>
    </test>  
</suite>